#: panels/bluetooth/cc-bluetooth-panel.c:352
msgid "Yes"
msgstr "ແມ່ນ"

#: panels/bluetooth/cc-bluetooth-panel.c:352
msgid "No"
msgstr "ບໍ່"

#: panels/bluetooth/cc-bluetooth-panel.c:466
msgid "Bluetooth is disabled"
msgstr "ບຣູທຸດ໌ຖຶກປິດໃຊ້ງານ"

#: panels/bluetooth/cc-bluetooth-panel.c:471
msgid "Bluetooth is disabled by hardware switch"
msgstr "ບຣູທຸດ໌ຖຶກປິດໃຊ້ງານຈາກການສະຫລັບຮາດແວຣ໌"

#: panels/bluetooth/cc-bluetooth-panel.c:475
msgid "No Bluetooth adapters found"
msgstr "ບໍ່ພົບອະແດັບເຕີ້ສຳລັບບຣູທຸດ໌"

#: panels/bluetooth/cc-bluetooth-panel.c:602
msgid "Visibility"
msgstr "ການເຫັນໄກ"

#: panels/bluetooth/cc-bluetooth-panel.c:606
#, c-format
msgid "Visibility of “%s”"
msgstr "ການເຫັນໄກຂອງ “%s”"

#: panels/bluetooth/cc-bluetooth-panel.c:650
#, c-format
msgid "Remove '%s' from the list of devices?"
msgstr "ຕ້ອງການເອົາ '%s' ອອກຈາກລາຍການອຸປປະກອນບໍ່?"

#: panels/bluetooth/cc-bluetooth-panel.c:652
msgid ""
"If you remove the device, you will have to set it up again before next use."
msgstr ""
"ຫາກເຈົ້າເອາອຸບປະກອນນີ້ອອກ, "
"ເຈົ້າຈະຕ້ອງຕັ້ງຄ່າມັນໃໝ່ອີກເທື່ອໜຶ່ງກ່ອນໃຊ້ງານເທື່ອຕໍ່ຈາກນີ້ໄປ."

#: panels/bluetooth/bluetooth.ui.h:1
msgid "Set Up New Device"
msgstr "ຕັ້ງຄ່າອຸບປະກອນໃໝ່"

#: panels/bluetooth/bluetooth.ui.h:2
msgid "Remove Device"
msgstr "ອຸປະກອນຖອດສຽບ"

#: panels/bluetooth/bluetooth.ui.h:3
msgid "Connection"
msgstr "ການເຊື່ອມຕໍ່"

#: panels/bluetooth/bluetooth.ui.h:4
msgid "page 1"
msgstr "ໜ້າ 1"

#: panels/bluetooth/bluetooth.ui.h:5
msgid "page 2"
msgstr "ໜ້າ 2"

#: panels/bluetooth/bluetooth.ui.h:6
msgid "Paired"
msgstr "ຈັບຄູ່ແລ້ວ"

#: panels/bluetooth/bluetooth.ui.h:7
msgid "Type"
msgstr "ປະເພດ"

#: panels/bluetooth/bluetooth.ui.h:8
msgid "Address"
msgstr "ທີ່ຢູ່"

#: panels/bluetooth/bluetooth.ui.h:9
msgid "Mouse and Touchpad Settings"
msgstr "ການຕັ້ງຄ່າ ເມົ້າ ແລະ ທັຊແພດ"

#: panels/bluetooth/bluetooth.ui.h:10
msgid "Sound Settings"
msgstr "ການຕັ້ງຄ່າ ສຽງ"

#: panels/bluetooth/bluetooth.ui.h:11
msgid "Keyboard Settings"
msgstr "ການຕັ້ງຄ່າ ແປ້ນພິມ"

#: panels/bluetooth/bluetooth.ui.h:12
msgid "Send Files..."
msgstr "ສົ່ງໄຟລ໌..."

#: panels/bluetooth/bluetooth.ui.h:13
msgid "Browse Files..."
msgstr "ເອີ້ນຫາໄຟລ໌..."

#: generate_additional_files.py:50
msgid "Bluetooth"
msgstr "ບຣູທຸດ໌"

#: generate_additional_files.py:50
msgid "Configure Bluetooth settings"
msgstr "ກຳໜົດຄ່າ ການຕັ້ງຄ່າ ບຣູທຸດ໌"
